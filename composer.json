{
  "name": "mleczakm/pulpo18bundle",
  "type": "symfony-bundle",
  "description": "Pulpo18 package",
  "keywords": [
    "doctrine",
    "documentation",
    "uml",
    "schema",
    "graph",
    "symfony",
    "bundle"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "MichaÅ‚ Mleczko",
      "email": "kontakt@michalmleczko.waw.pl"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mleczakm/platform-based"
    }
  ],
  "require": {
    "php"                           : ">=5.3.3",
    "symfony/dependency-injection"  : "~2.3|~3.0",
    "symfony/framework-bundle"      : "~2.3|~3.0",
    "mleczakm/platform-based"       : "dev-master",
    "symfony/console": "^2.3 || ^3.0"
  },
  "autoload": {
    "psr-4": { "mleczakm\\Pulpo18Bundle\\": "src/" }
  },
  "extra": {
    "platform-installer": {
      "darwin_64": [
        {
          "url": "http://downloads.pulpo18.com/1.1.0.47/Pulpo-1.1.0.47-MacOS-all-64bit.zip",
          "dir": "pulpo"
        }
      ],
      "win_32": [
        {
          "url": "http://downloads.pulpo18.com/1.1.0.47/Pulpo-1.1.0.47-Windows-all-32bit.zip",
          "dir": "pulpo"
        }
      ],
      "linux_32": [
        {
          "url": "http://downloads.pulpo18.com/1.1.0.47/Pulpo-1.1.0.47-Linux-all-32bit.zip",
          "dir": "pulpo"
        }
      ],
      "linux_64": [
        {
          "url": "http://downloads.pulpo18.com/1.1.0.47/Pulpo-1.1.0.47-Linux-all-64bit.zip",
          "dir": "pulpo"
        }
      ]
    }
  }
} 
